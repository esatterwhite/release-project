{
  "name": "@esatterwhite/release-project",
  "version": "0.0.0-development",
  "description": "Test release project",
  "main": "index.js",
  "scripts": {
    "test": "tap",
    "lint": "eslint ./",
    "lint:fix": "npm run lint -- --fix",
    "semantic-release": "semantic-release",
    "multi-semantic-release": "multi-semantic-release"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@esatterwhite/eslint-config-reasonable",
    "ignorePatterns": [
      "node_modules/",
      "coverage/",
      ".nyc_output/",
      "env/",
      "doc/"
    ],
    "rules": {
      "object-shorthand": 0,
      "no-unused-vars": ["error", {
        "varsIgnorePattern": "_"
      }],
      "quote-props": [
        2,
        "as-needed"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/esatterwhite/release-project.git"
  },
  "keywords": [
    "semantic"
  ],
  "workspaces": [
    "packages/semantic-release-docker",
    "services/api-service"
  ],
  "author": "Eric Satterwhite <esatterwhite@wir.rr.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/esatterwhite/release-project/issues"
  },
  "homepage": "https://github.com/esatterwhite/release-project#readme",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "multi-semantic-release": "^2.1.0",
    "semantic-release": "^17.1.1",
    "tap": "^14.10.7"
  },
  "tap": {},
  "dependencies": {
    "@esatterwhite/eslint-config-reasonable": "^1.0.2",
    "eslint": "^7.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-sensible": "^2.2.0"
  }
}
