{
  "name": "@codedependant/semantic-release-docker",
  "private": false,
  "version": "1.6.4",
  "description": "docker package",
  "main": "index.js",
  "scripts": {
    "test": "docker-compose -f compose/base.yml -f compose/test.yml up --exit-code-from semantic-release --force-recreate --remove-orphans",
    "start": "docker-compose -f compose/base.yml -f compose/dev.yml up --force-recreate --remove-orphans --build",
    "stop": "docker-compose -f compose/base.yml -f compose/dev.yml down",
    "tap": "tap"
  },
  "keywords": [
    "semantic-release"
  ],
  "author": "Eric Satterwhite <esatterwhite@wi.rr.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/esatterwhite/release-project.git",
    "directory": "packages/semantic-release-docker"
  },
  "tap": {
    "esm": false,
    "ts": false,
    "jsx": false,
    "timeout": 60,
    "browser": false,
    "check-coverage": true,
    "lines": 95,
    "branches": 95,
    "statements": 95,
    "functions": 95,
    "files": [
      "test/unit",
      "test/integration"
    ],
    "coverage-report": [
      "text",
      "text-summary",
      "json",
      "html"
    ],
    "nyc-arg": [
      "--exclude=coverage/",
      "--exclude=test/"
    ]
  },
  "devDependencies": {
    "@esatterwhite/eslint-config-reasonable": "^1.0.2",
    "eslint": "^7.4.0",
    "sinon": "^9.0.2",
    "supertest": "^4.0.2",
    "tap": "^14.10.7"
  },
  "dependencies": {
    "@semantic-release/github": "^7.0.7",
    "@semantic-release/error": "^2.2.0",
    "semantic-release": "^17.1.1",
    "debug": "^4.1.1",
    "execa": "^4.0.2",
    "semver": "^7.3.2"
  },
  "peerDependencies": {},
  "optionalDependencies": {}
}
